{
  "name": "microsoft-partnercenter",
  "version": "3.1.0",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "run-s -l clean test tsc copy",
    "build:check": "tsc --noEmit",
    "clean": "rimraf coverage dist tmp",
    "copy": "cpy package.json dist && cpy README.md dist",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "postinstall": "[ -d '.husky' ] && husky install || true",
    "qa": "run-s -l lint test clean build:check",
    "semantic-release": "semantic-release",
    "test": "jest",
    "tsc": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "microsoft-partnercenter-node",
    "microsoft",
    "partner center",
    "rest api",
    "rest"
  ],
  "author": "Josh Stout <joshstout@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenion/microsoft-partnercenter-node"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@pliancy/eslint-config-ts": "^0.0.5",
    "@pliancy/semantic-release-config-npm": "^2.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "commitizen": "^4.2.4",
    "cpy-cli": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "gh-pages": "^2.2.0",
    "husky": "^7.0.1",
    "jest": "^25.5.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
