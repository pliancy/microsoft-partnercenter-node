{
  "name": "microsoft-partnercenter",
  "version": "3.0.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint -f pretty src/",
    "test": "npm run lint && jest",
    "docs": "rimraf ./docs && typedoc src/ --out docs --mode modules --module commonjs --target es6 --excludePrivate && touch ./docs/.nojekyll && gh-pages -d docs -t",
    "build": "npm run clean && npm run test && tsc -p tsconfig.release.json",
    "prepare": "npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "microsoft-partnercenter-node",
    "microsoft",
    "partner center",
    "rest api",
    "rest"
  ],
  "author": "Josh Stout <joshstout@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenion/microsoft-partnercenter-node"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^16.0.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "jest": "^25.5.2",
    "lint-staged": "^10.2.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  }
}
